# Replace <directory-path> with the path where you created folders earlier
DATA_FOLDER=~/projects/n8n-docker-caddy

# The top level domain to serve from, this should be the same as the subdomain you created above
DOMAIN_NAME=10.26.7.210

# The subdomain to serve from
# SUBDOMAIN=n8n

# DOMAIN_NAME and SUBDOMAIN combined decide where n8n will be reachable from
# above example would result in: https://n8n.example.com

# Optional timezone to set which gets used by Cron-Node by default
# If not set New York time will be used
GENERIC_TIMEZONE=Europe/Berlin

# The email address to use for the SSL certificate creation
SSL_EMAIL=example@example.com

N8N_SECURE_COOKIE=false

#evolution api
#postgressql
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI='postgresql://user:pass@postgres:5432/evolution?schema=public'

# Redis Cache enabled
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://redis:6379/6

AUTHENTICATION_API_KEY=chave-secreta

CONFIG_SESSION_PHONE_VERSION=2.3000.1023715232